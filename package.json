{
  "name": "sfdx-metadata-patcher",
  "description": "A Salesforce DX Plugin to hotpatch metadata before deployments",
  "version": "0.0.23",
  "author": "Raffaele Fioratto",
  "bugs": "https://github.com/ImperiumMaximus/sfdx-metadata-patcher/issues",
  "dependencies": {
    "@oclif/command": "^1.8.18",
    "@oclif/config": "^1.18.4",
    "@oclif/errors": "^1.3.6",
    "@salesforce/command": "^4",
    "@salesforce/core": "^2",
    "@salesforce/source-deploy-retrieve": "^4.0.0",
    "@types/cli-progress": "^3.11.0",
    "@types/micromatch": "^4.0.1",
    "chromedriver": ">=107.0.0",
    "cli-progress": "^3.11.2",
    "common-substrings": "^3.0.1",
    "csv": "^5.5.0",
    "exceljs": "^4.2.1",
    "fast-fuzzy": "^1.11.2",
    "json-query": "^2.2.2",
    "jszip": "^3.10.0",
    "line-reader": "^0.4.0",
    "micromatch": "^4.0.2",
    "pino": "^6.11.1",
    "pino-pretty": "^9.1.1",
    "prompts": "^2.4.1",
    "salesforce-alm": "^54.8.2",
    "selenium-webdriver": "^4.4.0",
    "sqlstring": "^2.3.2",
    "tmp": "^0.2.1",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^2",
    "@oclif/test": "^1",
    "@salesforce/dev-config": "1.4.1",
    "@salesforce/ts-sinon": "^1.3.0",
    "@types/chai": "^4",
    "@types/chromedriver": "^81.0.1",
    "@types/line-reader": "^0.0.33",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/pino": "^6.3.5",
    "@types/pino-pretty": "^5.0.0",
    "@types/pino-std-serializers": "^4.0.0",
    "@types/prompts": "^2.0.14",
    "@types/selenium-webdriver": "^4.1.5",
    "@types/sqlstring": "^2.3.0",
    "@types/tmp": "^0.2.1",
    "@types/xml2js": "^0.4.9",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/eslint-plugin-tslint": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "chai": "^4",
    "eslint": "^8.11.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^39.0.6",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "tslint": "^6",
    "typescript": "4.3.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/cfg",
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/ImperiumMaximus/sfdx-metadata-patcher",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "topics": {
      "mdata": {
        "description": "Commands to manipulate Salesforce Metadata."
      }
    },
    "hooks": {
      "predeploy": "./lib/hooks/predeploy/mdataPatch"
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "ImperiumMaximus/sfdx-metadata-patcher",
  "scripts": {
    "lint": "eslint -c .eslintrc.js --ext .ts ./src",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint -c .eslintrc.js --ext .ts ./src",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\" --timeout 180000",
    "coverage": "nyc report --reporter text-lcov --extension .ts > coverage.lcov",
    "version": "oclif-dev readme && git add README.md"
  }
}
